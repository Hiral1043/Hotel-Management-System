<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_hotel_hotel" model="ir.sequence">
            <field name="name">Hotel</field>
            <field name="code">hotel.hotel</field>
            <field name="prefix">H</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hotel_room" model="ir.sequence">
            <field name="name">Hotel Room</field>
            <field name="code">hotel.rooms</field>
            <field name="prefix">HR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    
    <record id="seq_hotel_guest" model="ir.sequence">
            <field name="name">Hotel Room Guest</field>
            <field name="code">hotel.guest</field>
            <field name="prefix">G</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hotel_room_booking" model="ir.sequence">
            <field name="name">Hotel Room Booking</field>
            <field name="code">hotel.rooms.booking</field>
            <field name="prefix">HB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        


        <record id="seq_restaurant_order" model="ir.sequence">
            <field name="name">Restaurant</field>
            <field name="code">restaurant.order.lines</field>
            <field name="prefix">ORD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
 
   
    </data>
    </odoo>